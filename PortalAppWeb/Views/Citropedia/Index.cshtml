
@{
    ViewBag.Title = "Citropedia - Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script>
    $(function () {
        ucSearch.ChangeImgLogo('Content/img/logo8_citropedia.png');
        ucSearch.SetHrefLogo('#Home');
    });
</script>

<div id="Home" class="Citropedia PagesDisplayNone" style="display:none;">
    @{
        Html.RenderPartial("Pages/UC_Home");
    }
</div>

<div id="Search" class="container-fluid search_page_template PagesDisplayNone" style="display:none;">
    @{
        Html.RenderPartial("../Shared/UserControls/UC_Search");
    }
</div>


@section sidebar {
    @{
        Html.RenderPartial("UserControls/UC_Menu");
    }
}

@section footer {
    @{
        Html.RenderPartial("UserControls/UC_Footer");
    }
}